{
  "compilerOptions": {
    "strict": true,
    "noEmit": true,
    "allowJs": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "isolatedModules": true,
    "target": "ESNext",
    "lib": ["ESNext"],
    "resolveJsonModule": true,
    "moduleResolution": "Node",
    "module": "ESNext",
    "esModuleInterop": true
  },
  "include": ["algorithm", "data-structure"]
}
